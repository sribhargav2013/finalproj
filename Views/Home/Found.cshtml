    <!-- Report a Found Pet Form with Bootstrap Form Styles (customize as needed) -->
    <div class="container mt-4">
        <h2>Report a Pet</h2>
        <p class="lead">(Any unknown details can be left empty)</p>
        <div class="report-form">
            <form id="foundPetForm">
                <div class="form-group lead">
                    <label for="recordType">Record Type</label>
                    <select class="form-control" id="recordType" name="recordType">
                        <option value="FOUND">Found</option>
                        <option value="LOST">LOST</option>
                        <option value="ADOPTABLE">Adoptable</option>
                    </select>
                </div>
                <div class="lead">
                    <div class="form-group ">
                        <label for="location">Location</label>
                        <input type="text" class="form-control" id="location" name="location">
                    </div>
                    <div class="form-group">
                        <label for="name">Name</label>
                        <input type="text" class="form-control" id="name" name="name">
                    </div>
                    <div class="form-group">
                        <label for="type">Animal Type</label>
                        <input type="text" class="form-control" id="type" name="type">
                    </div>
                    <div class="form-group">
                        <label for="age">Age</label>
                        <input type="text" class="form-control" id="age" name="age">
                    </div>
                    <div class="form-group">
                        <label for="color">Animal Color</label>
                        <input type="text" class="form-control" id="color" name="color">
                    </div>
                    <div class="form-group">
                        <label for="date">Date</label>
                        <input type="text" class="form-control" id="date" name="date">
                    </div>
                    <div class="form-group">
                        <label for="Dtype">Date Type</label>
                        <input type="text" class="form-control" id="Dtype" name="Dtype">
                    </div>

                </div>
                <!-- Add more form fields as needed -->
                <div class="text-center" style="margin-bottom:15px">
                    <button type="submit" class="btn btn-primary ">Submit</button>
                </div>
            </form>
        </div>
